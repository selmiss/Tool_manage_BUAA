rd : '',\n            newPassword : '',\n            confirmPassword : ''\n        }\n    },\n    methods:{\n        confirm(){\n            if(this.confirmPassword == this.newPassword){\n                axios({\n                method:'post',\n                url: 'user/reSetPwd',\n                data: {\n                    uid: localStorage.getItem('uid'),\n                    old_pwd: this.password,\n                    new_pwd: this.newPassword\n                    }\n                })\n                .then( res => {\n                    console.log(res);\n                    if(res.data.error_code == 0){\n                        alert('修改成功！请重新登录');\n                    this.$router.push('/');\n                    }\n                    if(res.data.error_code == 2){\n                        alert('原密码错误！');\n                    }\n                    if(res.data.error_code == 3){\n                        alert('新密码格式错误！');\n                    }\n                    if(res.data.error_code == 4){\n                        alert('用户不存在！');\n                    }\n                })\n            }\n            else alert('两次新密码输入不一致，请重新输入！')\n        },\n    }\n})\n</script>\n\n<style>\n    .title {\n    color: #06c;\n    margin-bottom: 20px;\n    font-family: 微软雅黑;\n    word-wrap: break-word;\n    /* text-align: left; */\n    font-size: 25px;\n    font-weight: bold;\n  }\n</style>"],"sourceRoot":"src/views/Entry"}]}