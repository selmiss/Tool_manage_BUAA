r},\n    mounted(){\n        this.loadTable();\n    },\n    data(){\n        return {\n            userId : '',\n            studentId: '',\n            email :'',\n            name : '',\n        }\n    },\n    methods:{\n        loadTable(){\n            this.userId = this.$store.state.UID;\n            // console.log(localStorage.getItem('Mail'));\n            console.log(localStorage.getItem('uid'));\n            axios({\n                method:'post',\n                url: 'http://121.4.160.157/user/getInfo',\n                data: {\n                    uid: localStorage.getItem('uid')\n                }\n            })\n            .then( res => {\n                console.log(res);\n                if(res.data.error_code == 0) {\n                    this.name = res.data.name,\n                    this.email = res.data.acc,\n                    this.studentId = res.data.studentId\n                }\n            })\n        },\n        confirm(){\n            console.log(localStorage.getItem('uid'));\n            axios({\n                method:'post',\n                url: 'http://121.4.160.157/manager/putInfo',\n                data: {\n                    uid: localStorage.getItem('uid'),\n                    studentId: this.studentId,\n                    name: this.name,\n                }\n            })\n            .then( res => {\n                console.log(res);\n                if(res.data.error_code == 0) {\n                    alert('修改成功！')\n                }\n            })\n        },\n    }\n})\n</script>\n\n<style>\n    .title {\n    color: rgb(204, 122, 0);\n    margin-bottom: 20px;\n    font-family: 微软雅黑;\n    word-wrap: break-word;\n    /* text-align: left; */\n    font-size: 25px;\n    font-weight: bold;\n  }\n</style>"],"sourceRoot":"src/views/Entry"}]}